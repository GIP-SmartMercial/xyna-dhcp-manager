{
    "name": "xyna",
    "version": "0.0.1",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "start:ssl": "ng serve --ssl=true",
        "start2:ssl": "node --max_old_space_size=2048 ./node_modules/@angular/cli/bin/ng serve --ssl=true",
        "start:de-DE": "ng serve --configuration=de-DE",
        "start:de-DE:ssl": "ng serve --ssl=true --configuration=de-DE",
        "start2:de-DE:ssl": "node --max_old_space_size=2048 ./node_modules/@angular/cli/bin/ng serve --ssl=true --configuration=de-DE",
        "build": "ng build xyna",
        "build:de-DE": "ng build xyna --configuration=de-DE",
        "release": "ng build xyna --prod --extract-licenses",
        "release:de-DE": "ng build xyna --prod --extract-licenses --i18n-file ./project/xyna/locale/messages.de-DE.xlf --i18n-format xlf --i18n-locale de-DE",
        "release:en-US": "ng build xyna --prod --extract-licenses --i18n-file ./project/xyna/locale/messages.en-US.xlf --i18n-format xlf --i18n-locale en-US",
        "test": "ng test xyna --sourcemaps=false",
        "lint": "ng lint xyna --type-check --tslint-config ./project/xyna/src/app/zeta/lint/tslint.json",
        "tsc": "tsc",
        "compile-lint-rules": "tsc @project/xyna/src/app/zeta/lint/ts-rule-files.txt",
        "extract-i18n": "ng xi18n xyna --i18n-format xlf --output-path ./locale --i18n-locale en-US && ng run xyna:xliffmerge"
    },
    "private": true,
    "dependencies": {
        "web-animations-js": "2.3.2",
        "core-js": "3.4.2",
        "hammerjs": "2.0.8",
        "zone.js": "0.10.2",
        "rxjs": "6.5.3",
        "three": "0.109.0",
        "@angular/material": "8.2.3",
        "@angular/cdk": "8.2.3",
        "@angular/animations": "8.2.14",
        "@angular/common": "8.2.14",
        "@angular/compiler": "8.2.14",
        "@angular/core": "8.2.14",
        "@angular/forms": "8.2.14",
        "@angular/platform-browser": "8.2.14",
        "@angular/platform-browser-dynamic": "8.2.14",
        "@angular/router": "8.2.14",
        "@angular/service-worker": "8.2.14",
        "@ngx-i18nsupport/tooling": "^8.0.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "0.803.23",
        "@angular/cli": "8.3.19",
        "@angular/compiler-cli": "8.2.14",
        "@angular/language-service": "8.2.14",
        "@ngx-i18nsupport/ngx-i18nsupport": "^1.1.6",
        "@types/node": "12.7.3",
        "codelyzer": "4.5.0",
        "ts-node": "7.0.1",
        "tslint": "5.20.1",
        "tslint-eslint-rules": "5.4.0",
        "typescript": "3.5.3"
    }
}
